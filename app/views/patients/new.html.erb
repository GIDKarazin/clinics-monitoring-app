<h1>New Patient</h1>

<%= form_for @patient do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br>

  <%= f.label :birthdate %>
  <%= f.text_field :birthdate %><br>

  <%= f.label :email %>
  <%= f.text_field :email %><br>

  <%= f.label :phone %>
  <%= f.text_field :phone %><br>

  <%= f.label :address %>
  <%= f.text_field :address %><br>

  <%= f.submit "Create" %>
<% end %>