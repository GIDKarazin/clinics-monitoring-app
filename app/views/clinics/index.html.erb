<h1>All Clinics</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @clinics.each do |clinic| %>
      <tr>
        <td><%= clinic.id %></td>
        <td><%= clinic.name %></td>
        <td><%= clinic.email %></td>
        <td><%= clinic.phone %></td>
        <td><%= clinic.address %></td>
        <td><%= link_to 'Show', clinic_path(clinic) %></td>
        <td><%= link_to 'Edit', edit_clinic_path(clinic) %></td>
        <td><%= button_to 'Delete', clinic_path(clinic), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Clinic', new_clinic_path %>