<h1>All Clinics</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Address</th>
    </tr>
  </thead>
  <tbody>
    <% Clinic.all.each do |clinic| %>
      <tr>
        <td><%= clinic.id %></td>
        <td><%= clinic.name %></td>
        <td><%= clinic.email %></td>
        <td><%= clinic.phone %></td>
        <td><%= clinic.address %></td>
      </tr>
    <% end %>
  </tbody>
</table>